# Airflow Configuration
# Copy this file to .env and update the values

# Airflow UID (run 'id -u' to get your user ID)
AIRFLOW_UID=50000

# Fernet key for encrypting sensitive data (generate with: python -c "from cryptography.fernet import Fernet; print(Fernet.generate_key().decode())")
AIRFLOW_FERNET_KEY=YOUR_FERNET_KEY_HERE

# PostgreSQL password
POSTGRES_PASSWORD=airflow_db_password_change_me

# Airflow Admin User
AIRFLOW_ADMIN_USER=admin
AIRFLOW_ADMIN_PASSWORD=YOUR_ADMIN_PASSWORD
AIRFLOW_ADMIN_EMAIL=admin@example.com

# Additional pip packages (optional)
_PIP_ADDITIONAL_REQUIREMENTS=

# Azure Data Lake Storage
AZURE_STORAGE_ACCOUNT_NAME=gaaborotrinity
AZURE_STORAGE_ACCOUNT_KEY=your_storage_account_key_here
